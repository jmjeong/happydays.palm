    CHECKBOX "Display solar/lunar info in View" ID DispPrefExtraInfo
        AT (10 PrevBottom+3 AUTO AUTO)
*/
    BUTTON "OK" ID DispPrefOk AT (5 BOTTOM@59 40 AUTO)
